{% if items|length > 0 %}
<p class="dialog-title alert alert-warning txt-center">{{ questionMsg }}</p>
<div class="row">
    <p><a class="bb-revision-see-details" href="#" >{{ 'validate_cancel_see_details'|trans }}</a></p>
    <div class="bb-revision-list" style="display:none;">
        <div class="bb5-list-data-toolbar bb5-toolbar-order">
            <div class="row">
                <p class="bb5-toolbar-order-col col-bb5-60"><a href="#">{{ 'title'|trans }}</a></p>
                <p class="bb5-toolbar-order-col col-bb5-20"><a href="#">{{ 'author'|trans }}</i></a></p>
                <p class="bb5-toolbar-order-col col-bb5-20"><a href="#">{{ 'modified_at'|trans }}</i></a></p>
            </div>
        </div>
        <ul class="bb5-list-data bb5-list-display-list clearfix bb-revision-page-selector">
            <li class="bb5-list-data-item">
            {{ items }}
            </li>
        </ul>
    </div>
</div>
{% else %}
<p>{{ noContentMsg }}</p>
{% endif %}